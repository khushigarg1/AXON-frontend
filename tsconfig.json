{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*",
    "nativewind-env.d.ts"
, "theme.ts"  ],
  "exclude": [
    "node_modules",
    "**/__tests__/*"
  ],
  "extends": "expo/tsconfig.base"
}
